@model NCAAFootballPlayoffs.Models.User


@{
    ViewBag.Title = "NCAA Bracket - Home";
}

<h2>Home</h2>
@Html.ActionLink("My bracket", "Index", "Bracket")

@Html.ActionLink("DoStuff", "doWhatIWant", "Home")



<div class="row">
    <div class="col-sm-12">
        @if (Model != null) {
            <div>
                @Model.EmailAddress;
            </div>
            foreach (var username in Model.Usernames)
            {
                <div>
                    @Html.ActionLink(username.UsernameText, "Index", "Bracket", new { usernameID = username.UsernameID}, new { })
                </div>
            }
        }
        
    </div>
</div>